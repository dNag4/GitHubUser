<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.GitHubUser" parent="Theme.AppCompat.DayNight">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/grey_147</item>
        <item name="colorPrimaryVariant">@color/grey_200</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/backColor</item>
        <!-- Customize your theme here. -->

        <item name="backColor">@color/grey_200</item>
        <item name="textcolor">@color/black</item>
        <item name="cardcolor">@color/grey_239</item>
        <item name="tintcolor">#FFE1E1E1</item>
        <item name="iconcolor">@color/black</item>
        <item name="searchcolor">@color/grey_239</item>
        <item name="tabcolor">#FFDDDDDD</item>
        <item name="panecolor">#FFEFEFEF</item>
        <item name="splashcolor">#FFE6E6E6</item>
    </style>
</resources>